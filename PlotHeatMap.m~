load('corrMatrix.mat');
h = heatmap(corrMatrix, 1:5:29, 1:5:29);
% h.Title = 'Heatmap for 29 features';
h.XLabel = 'Feature ID (F_id)';
h.YLabel = 'Feature ID (F_id)';
